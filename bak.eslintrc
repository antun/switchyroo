{
  "globals": {},
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "plugins": [
  ],
  "rules": {
    "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
    "indent": [2, 2, {"SwitchCase": 1}],
    "curly": 2,
    "eqeqeq": 2,
    "no-redeclare": 2,
    "no-use-before-define": 1,
    "no-caller": 2,
    "block-scoped-var": 2,
    "comma-dangle": [2, "never"],
    "no-unused-vars": 2,
    "no-eq-null": 2,
    "guard-for-in": 2,
    "no-empty": 2,
    "no-trailing-spaces": 2,
    "no-eval": 2
  }
}
